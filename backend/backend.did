type AddConfiguredTokenRequest = record {
  token_address : text;
  network : text;
};
type AddressResponse = record {
  network : text;
  message : opt text;
  address : text;
  key_name : text;
  public_key_hex : text;
};
type BalanceRequest = record { token : opt text; account : text };
type BalanceResponse = record {
  decimals : opt nat8;
  token : opt text;
  pending : bool;
  network : text;
  block_ref : opt text;
  message : opt text;
  account : text;
  amount : opt text;
};
type ConfiguredExplorerResponse = record {
  token_url_template : opt text;
  network : text;
  address_url_template : text;
};
type ConfiguredRpcResponse = record { network : text; rpc_url : text };
type ConfiguredTokenResponse = record {
  decimals : nat64;
  name : text;
  token_address : text;
  network : text;
  symbol : text;
};
type NetworkModuleStatus = record {
  note : opt text;
  network : text;
  balance_ready : bool;
  transfer_ready : bool;
};
type RemoveConfiguredRpcRequest = record { network : text };
type Result = variant { Ok : ConfiguredTokenResponse; Err : WalletError };
type Result_1 = variant { Ok : BalanceResponse; Err : WalletError };
type Result_2 = variant { Ok : AddressResponse; Err : WalletError };
type Result_3 = variant { Ok : TransferResponse; Err : WalletError };
type Result_4 = variant { Ok; Err : WalletError };
type Result_5 = variant { Ok : bool; Err : WalletError };
type Result_6 = variant { Ok : opt principal; Err : WalletError };
type Result_7 = variant { Ok : ConfiguredRpcResponse; Err : WalletError };
type ServiceInfoResponse = record {
  owner : opt principal;
  note : opt text;
  version : text;
  caller : principal;
  paused : bool;
};
type TransferRequest = record {
  to : text;
  token : opt text;
  metadata : vec record { text; text };
  from : opt text;
  memo : opt text;
  nonce : opt text;
  amount : text;
};
type TransferResponse = record {
  tx_id : opt text;
  network : text;
  message : text;
  accepted : bool;
};
type WalletError = variant {
  Internal : text;
  Paused;
  InvalidInput : text;
  Forbidden;
  Unimplemented : record { network : text; operation : text };
};
type WalletNetworkInfoResponse = record {
  id : text;
  default_rpc_url : opt text;
  primary_symbol : text;
  supports_balance : bool;
  shared_address_group : text;
  supports_send : bool;
  address_family : text;
};
service : () -> {
  add_configured_token : (AddConfiguredTokenRequest) -> (Result);
  aptos_mainnet_get_balance_apt : (BalanceRequest) -> (Result_1);
  aptos_mainnet_get_balance_token : (BalanceRequest) -> (Result_1);
  aptos_mainnet_request_address : () -> (Result_2);
  aptos_mainnet_transfer_apt : (TransferRequest) -> (Result_3);
  aptos_mainnet_transfer_token : (TransferRequest) -> (Result_3);
  arbitrum_get_balance_erc20 : (BalanceRequest) -> (Result_1);
  arbitrum_get_balance_eth : (BalanceRequest) -> (Result_1);
  arbitrum_request_address : () -> (Result_2);
  arbitrum_transfer_erc20 : (TransferRequest) -> (Result_3);
  arbitrum_transfer_eth : (TransferRequest) -> (Result_3);
  avalanche_get_balance_avax : (BalanceRequest) -> (Result_1);
  avalanche_get_balance_erc20 : (BalanceRequest) -> (Result_1);
  avalanche_request_address : () -> (Result_2);
  avalanche_transfer_avax : (TransferRequest) -> (Result_3);
  avalanche_transfer_erc20 : (TransferRequest) -> (Result_3);
  base_get_balance_erc20 : (BalanceRequest) -> (Result_1);
  base_get_balance_eth : (BalanceRequest) -> (Result_1);
  base_request_address : () -> (Result_2);
  base_transfer_erc20 : (TransferRequest) -> (Result_3);
  base_transfer_eth : (TransferRequest) -> (Result_3);
  bitcoin_get_balance_btc : (BalanceRequest) -> (Result_1);
  bitcoin_request_address : () -> (Result_2);
  bitcoin_transfer_btc : (TransferRequest) -> (Result_3);
  bsc_get_balance_bep20 : (BalanceRequest) -> (Result_1);
  bsc_get_balance_bnb : (BalanceRequest) -> (Result_1);
  bsc_request_address : () -> (Result_2);
  bsc_transfer_bep20 : (TransferRequest) -> (Result_3);
  bsc_transfer_bnb : (TransferRequest) -> (Result_3);
  configured_explorer : (text) -> (opt ConfiguredExplorerResponse) query;
  configured_rpcs : () -> (vec ConfiguredRpcResponse) query;
  configured_tokens : (text) -> (vec ConfiguredTokenResponse) query;
  ethereum_get_balance_erc20 : (BalanceRequest) -> (Result_1);
  ethereum_get_balance_eth : (BalanceRequest) -> (Result_1);
  ethereum_request_address : () -> (Result_2);
  ethereum_transfer_erc20 : (TransferRequest) -> (Result_3);
  ethereum_transfer_eth : (TransferRequest) -> (Result_3);
  get_owner : () -> (opt principal) query;
  internet_computer_get_balance_icp : (BalanceRequest) -> (
      Result_1,
    ) composite_query;
  internet_computer_get_balance_icrc : (BalanceRequest) -> (
      Result_1,
    ) composite_query;
  internet_computer_transfer_icp : (TransferRequest) -> (Result_3);
  internet_computer_transfer_icrc : (TransferRequest) -> (Result_3);
  is_paused : () -> (bool) query;
  near_mainnet_get_balance_near : (BalanceRequest) -> (Result_1);
  near_mainnet_get_balance_nep141 : (BalanceRequest) -> (Result_1);
  near_mainnet_request_address : () -> (Result_2);
  near_mainnet_transfer_near : (TransferRequest) -> (Result_3);
  near_mainnet_transfer_nep141 : (TransferRequest) -> (Result_3);
  okx_get_balance_erc20 : (BalanceRequest) -> (Result_1);
  okx_get_balance_okb : (BalanceRequest) -> (Result_1);
  okx_request_address : () -> (Result_2);
  okx_transfer_erc20 : (TransferRequest) -> (Result_3);
  okx_transfer_okb : (TransferRequest) -> (Result_3);
  optimism_get_balance_erc20 : (BalanceRequest) -> (Result_1);
  optimism_get_balance_eth : (BalanceRequest) -> (Result_1);
  optimism_request_address : () -> (Result_2);
  optimism_transfer_erc20 : (TransferRequest) -> (Result_3);
  optimism_transfer_eth : (TransferRequest) -> (Result_3);
  pause : () -> (Result_4);
  polygon_get_balance_erc20 : (BalanceRequest) -> (Result_1);
  polygon_get_balance_pol : (BalanceRequest) -> (Result_1);
  polygon_request_address : () -> (Result_2);
  polygon_transfer_erc20 : (TransferRequest) -> (Result_3);
  polygon_transfer_pol : (TransferRequest) -> (Result_3);
  remove_configured_rpc : (RemoveConfiguredRpcRequest) -> (Result_5);
  remove_configured_token : (AddConfiguredTokenRequest) -> (Result_5);
  rotate_owner : (principal) -> (Result_6);
  sepolia_get_balance_erc20 : (BalanceRequest) -> (Result_1);
  sepolia_get_balance_eth : (BalanceRequest) -> (Result_1);
  sepolia_request_address : () -> (Result_2);
  sepolia_transfer_erc20 : (TransferRequest) -> (Result_3);
  sepolia_transfer_eth : (TransferRequest) -> (Result_3);
  service_info : () -> (ServiceInfoResponse) query;
  set_configured_rpc : (ConfiguredRpcResponse) -> (Result_7);
  solana_get_balance_sol : (BalanceRequest) -> (Result_1);
  solana_get_balance_spl : (BalanceRequest) -> (Result_1);
  solana_request_address : () -> (Result_2);
  solana_testnet_get_balance_sol : (BalanceRequest) -> (Result_1);
  solana_testnet_get_balance_spl : (BalanceRequest) -> (Result_1);
  solana_testnet_request_address : () -> (Result_2);
  solana_testnet_transfer_sol : (TransferRequest) -> (Result_3);
  solana_testnet_transfer_spl : (TransferRequest) -> (Result_3);
  solana_transfer_sol : (TransferRequest) -> (Result_3);
  solana_transfer_spl : (TransferRequest) -> (Result_3);
  sui_mainnet_get_balance_sui : (BalanceRequest) -> (Result_1);
  sui_mainnet_get_balance_token : (BalanceRequest) -> (Result_1);
  sui_mainnet_request_address : () -> (Result_2);
  sui_mainnet_transfer_sui : (TransferRequest) -> (Result_3);
  sui_mainnet_transfer_token : (TransferRequest) -> (Result_3);
  supported_networks : () -> (vec NetworkModuleStatus) query;
  ton_mainnet_get_balance_jetton : (BalanceRequest) -> (Result_1);
  ton_mainnet_get_balance_ton : (BalanceRequest) -> (Result_1);
  ton_mainnet_request_address : () -> (Result_2);
  ton_mainnet_transfer_jetton : (TransferRequest) -> (Result_3);
  ton_mainnet_transfer_ton : (TransferRequest) -> (Result_3);
  tron_get_balance_trc20 : (BalanceRequest) -> (Result_1);
  tron_get_balance_trx : (BalanceRequest) -> (Result_1);
  tron_request_address : () -> (Result_2);
  tron_transfer_trc20 : (TransferRequest) -> (Result_3);
  tron_transfer_trx : (TransferRequest) -> (Result_3);
  unpause : () -> (Result_4);
  wallet_networks : () -> (vec WalletNetworkInfoResponse) query;
  whoami : () -> (principal) query;
}

type AddressRequest = record { account_tag : opt text; index : opt nat32 };
type AddressResponse = record {
  network : text;
  account_tag : opt text;
  message : opt text;
  address : text;
  key_name : text;
  index : nat32;
  public_key_hex : text;
};
type BalanceRequest = record { token : opt text; account : text };
type BalanceResponse = record {
  decimals : opt nat8;
  token : opt text;
  pending : bool;
  network : text;
  block_ref : opt text;
  message : opt text;
  account : text;
  amount : opt text;
};
type ConfiguredExplorerResponse = record {
  token_url_template : opt text;
  network : text;
  address_url_template : text;
};
type ConfiguredTokenResponse = record {
  decimals : nat64;
  name : text;
  token_address : text;
  network : text;
  symbol : text;
};
type NetworkModuleStatus = record {
  note : opt text;
  network : text;
  balance_ready : bool;
  transfer_ready : bool;
};
type Result = variant { Ok : BalanceResponse; Err : WalletError };
type Result_1 = variant { Ok : TransferResponse; Err : WalletError };
type Result_2 = variant { Ok : AddressResponse; Err : WalletError };
type Result_3 = variant { Ok; Err : WalletError };
type Result_4 = variant { Ok : opt principal; Err : WalletError };
type ServiceInfoResponse = record {
  owner : opt principal;
  note : opt text;
  version : text;
  caller : principal;
  paused : bool;
};
type TransferRequest = record {
  to : text;
  token : opt text;
  metadata : vec record { text; text };
  from : opt text;
  memo : opt text;
  nonce : opt text;
  amount : text;
};
type TransferResponse = record {
  tx_id : opt text;
  network : text;
  message : text;
  accepted : bool;
};
type WalletError = variant {
  Internal : text;
  Paused;
  InvalidInput : text;
  Forbidden;
  Unimplemented : record { network : text; operation : text };
};
type WalletNetworkInfoResponse = record {
  id : text;
  default_rpc_url : opt text;
  primary_symbol : text;
  supports_balance : bool;
  supports_send : bool;
};
service : () -> {
  aptos_get_balance_apt : (BalanceRequest) -> (Result) query;
  aptos_get_balance_token : (BalanceRequest) -> (Result) query;
  aptos_transfer_apt : (TransferRequest) -> (Result_1);
  aptos_transfer_token : (TransferRequest) -> (Result_1);
  arb_get_balance_erc20 : (BalanceRequest) -> (Result);
  arb_get_balance_eth : (BalanceRequest) -> (Result);
  arb_transfer_erc20 : (TransferRequest) -> (Result_1);
  arb_transfer_eth : (TransferRequest) -> (Result_1);
  avax_get_balance_avax : (BalanceRequest) -> (Result);
  avax_get_balance_erc20 : (BalanceRequest) -> (Result);
  avax_transfer_avax : (TransferRequest) -> (Result_1);
  avax_transfer_erc20 : (TransferRequest) -> (Result_1);
  base_get_balance_erc20 : (BalanceRequest) -> (Result);
  base_get_balance_eth : (BalanceRequest) -> (Result);
  base_transfer_erc20 : (TransferRequest) -> (Result_1);
  base_transfer_eth : (TransferRequest) -> (Result_1);
  bsc_get_balance_bep20 : (BalanceRequest) -> (Result);
  bsc_get_balance_bnb : (BalanceRequest) -> (Result);
  bsc_transfer_bep20 : (TransferRequest) -> (Result_1);
  bsc_transfer_bnb : (TransferRequest) -> (Result_1);
  btc_get_balance_btc : (BalanceRequest) -> (Result) query;
  btc_request_address : (AddressRequest) -> (Result_2);
  btc_transfer_btc : (TransferRequest) -> (Result_1);
  configured_explorer : (text) -> (opt ConfiguredExplorerResponse) query;
  configured_tokens : (text) -> (vec ConfiguredTokenResponse) query;
  eth_get_balance_erc20 : (BalanceRequest) -> (Result);
  eth_get_balance_eth : (BalanceRequest) -> (Result);
  eth_request_address : (AddressRequest) -> (Result_2);
  eth_transfer_erc20 : (TransferRequest) -> (Result_1);
  eth_transfer_eth : (TransferRequest) -> (Result_1);
  get_owner : () -> (opt principal) query;
  icp_get_balance_icp : (BalanceRequest) -> (Result) query;
  icp_get_balance_icrc : (BalanceRequest) -> (Result) query;
  icp_transfer_icp : (TransferRequest) -> (Result_1);
  icp_transfer_icrc : (TransferRequest) -> (Result_1);
  is_paused : () -> (bool) query;
  near_get_balance_near : (BalanceRequest) -> (Result) query;
  near_get_balance_nep141 : (BalanceRequest) -> (Result) query;
  near_transfer_near : (TransferRequest) -> (Result_1);
  near_transfer_nep141 : (TransferRequest) -> (Result_1);
  okb_get_balance_erc20 : (BalanceRequest) -> (Result);
  okb_get_balance_okb : (BalanceRequest) -> (Result);
  okb_transfer_erc20 : (TransferRequest) -> (Result_1);
  okb_transfer_okb : (TransferRequest) -> (Result_1);
  op_get_balance_erc20 : (BalanceRequest) -> (Result);
  op_get_balance_eth : (BalanceRequest) -> (Result);
  op_transfer_erc20 : (TransferRequest) -> (Result_1);
  op_transfer_eth : (TransferRequest) -> (Result_1);
  pause : () -> (Result_3);
  polygon_get_balance_erc20 : (BalanceRequest) -> (Result);
  polygon_get_balance_pol : (BalanceRequest) -> (Result);
  polygon_transfer_erc20 : (TransferRequest) -> (Result_1);
  polygon_transfer_pol : (TransferRequest) -> (Result_1);
  rotate_owner : (principal) -> (Result_4);
  sepolia_get_balance_erc20 : (BalanceRequest) -> (Result);
  sepolia_get_balance_eth : (BalanceRequest) -> (Result);
  sepolia_request_address : (AddressRequest) -> (Result_2);
  sepolia_transfer_erc20 : (TransferRequest) -> (Result_1);
  sepolia_transfer_eth : (TransferRequest) -> (Result_1);
  service_info : () -> (ServiceInfoResponse) query;
  sol_get_balance_sol : (BalanceRequest) -> (Result);
  sol_get_balance_spl : (BalanceRequest) -> (Result);
  sol_request_address : (AddressRequest) -> (Result_2);
  sol_transfer_sol : (TransferRequest) -> (Result_1);
  sol_transfer_spl : (TransferRequest) -> (Result_1);
  solana_testnet_get_balance_sol : (BalanceRequest) -> (Result);
  solana_testnet_get_balance_spl : (BalanceRequest) -> (Result);
  solana_testnet_request_address : (AddressRequest) -> (Result_2);
  solana_testnet_transfer_sol : (TransferRequest) -> (Result_1);
  solana_testnet_transfer_spl : (TransferRequest) -> (Result_1);
  sui_get_balance_sui : (BalanceRequest) -> (Result) query;
  sui_get_balance_token : (BalanceRequest) -> (Result) query;
  sui_transfer_sui : (TransferRequest) -> (Result_1);
  sui_transfer_token : (TransferRequest) -> (Result_1);
  supported_networks : () -> (vec NetworkModuleStatus) query;
  ton_get_balance_jetton : (BalanceRequest) -> (Result) query;
  ton_get_balance_ton : (BalanceRequest) -> (Result) query;
  ton_transfer_jetton : (TransferRequest) -> (Result_1);
  ton_transfer_ton : (TransferRequest) -> (Result_1);
  trx_get_balance_trc20 : (BalanceRequest) -> (Result) query;
  trx_get_balance_trx : (BalanceRequest) -> (Result) query;
  trx_transfer_trc20 : (TransferRequest) -> (Result_1);
  trx_transfer_trx : (TransferRequest) -> (Result_1);
  unpause : () -> (Result_3);
  wallet_networks : () -> (vec WalletNetworkInfoResponse) query;
  whoami : () -> (principal) query;
}
